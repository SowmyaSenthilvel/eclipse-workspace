import java.util.*;
public class EmployeeDetails {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		/*
		 * EmployeePojo emp1=new EmployeePojo(124,"Ramya",34); EmployeePojo emp2=new
		 * EmployeePojo(125,"Vinoth",40); EmployeePojo emp3=new
		 * EmployeePojo(126,"Naresh",38); EmployeePojo emp4=new
		 * EmployeePojo(130,"Dinesh",42);
		 */
		ArrayList<EmployeePojo> alist=new ArrayList<EmployeePojo>();
		alist.add(new EmployeePojo(124,"Ramya",34));
		alist.add(new EmployeePojo(165,"Vinoth",40));
		alist.add(new EmployeePojo(186,"Swetha",32);
		alist.add(new EmployeePojo(145,"Ram",35);
		for(EmployeePojo s:alist) {
			System.out.println(s.getEmp_Id()+s.getEmp_Name()+s.getEmp_Age());
		}
		

	}

}
