
public class StringObj {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		StringObj s=new StringObj();
		String[] sd= {"Ramya","Preethi","Sahana"};
		s.joinWords(sd);

	}
	public void joinWords(String[] sd) {
		String str="";
		for(String st:sd) {
			System.out.println(st);
		}
		
	}

}
